using System;
using System.Console;
using Nemerle.Utility;

module Program
{
  Main() : void
  {
    def power(f, n) {
        match(n) {
            | 0 => (x => x)
            | 1 => f
            | _ => (x => f(power(f, n - 1)(x)))
        }
    }
    def g(k) { k + 1 }
    def i = power(g, 5)(3);
    WriteLine(i);
  }
}
