using System;

public class A
{
  public this(i : int)
  {
    InitValueOut(out _i, i);
    Ref(ref _i);
  }

  InitValueOut(v : out int, value : int) : void { v = value }
  Ref(x : ref int) : void { x++ }
  public I : int { get { _i } }
  private _i : int;
}

module Program
{
  Main() : void
  {
    def a = A(10);
    Console.WriteLine(a.I);
  }
}

