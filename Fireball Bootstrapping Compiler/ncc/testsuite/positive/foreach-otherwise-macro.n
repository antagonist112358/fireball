using System;
using System.Console;
using System.Collections.Generic;

module Program
{
  EmptyEnumerable() : IEnumerable[string]
  {
    def emptyList = List.[string]();
    foreach (i in emptyList)
      yield i;
  }
  
  Main() : void
  {
    // Empty list
    foreach (i in [] : list[int])
      if (i == 0)
        WriteLine(i);
      else
        WriteLine(i + 1);
    otherwise
      WriteLine("Otherwise1");
      
    foreach (i in [] : list[int])
      if (i == 0)
        WriteLine(i);
      else
        WriteLine(i + 1);
      
    // Empty array
    foreach (i in (array(0) : array[int]))
      when (i == 0)
        WriteLine(i);
    otherwise
      WriteLine("Otherwise2");
      
    foreach (i in (array(0) : array[int]))
      when (i == 0)
        WriteLine(i);
      
    // Empty List
    foreach (i in List.[int]())
    {
      while (i == 0)
        WriteLine(i);
    }
    otherwise
      WriteLine("Otherwise3");
      
    foreach (i in List.[int]())
      while (i == 0)
        WriteLine(i);
        
    // Empty iterator
    foreach (i in EmptyEnumerable())
      WriteLine(i);
    otherwise
      WriteLine("Otherwise4");        
    
    foreach (i in EmptyEnumerable())
      WriteLine(i);
    
    // foreach in foreach
    foreach (_ in EmptyEnumerable())
      foreach (i in EmptyEnumerable())
        WriteLine(i);
      otherwise
        WriteLine("Other1");
    otherwise
      WriteLine("Otherwise5");
      
    foreach (_ in EmptyEnumerable())
    {
      foreach (i in EmptyEnumerable())
        WriteLine(i);
    }
    otherwise
      WriteLine("Otherwise6");
      
    foreach (_ in EmptyEnumerable())
      foreach (i in EmptyEnumerable())
        WriteLine(i);
      otherwise
        WriteLine("Other2");
      
    // Complex cases
    if (1 == 2)
    {
      unless (false)
        foreach (_ in EmptyEnumerable())
          unless (false)
            WriteLine("Other3");
    }
    else
      foreach (_ in EmptyEnumerable())
        WriteLine("Other4");
    
    if (1 == 2)
      foreach (i in EmptyEnumerable())
        WriteLine(i);
    else
      foreach (i in EmptyEnumerable())
        WriteLine(i);
      otherwise
        WriteLine("Otherwise7");
    
    foreach (v in [] : list[int] with i)
      WriteLine("{0} {1}", v, i);
    otherwise
      WriteLine("Otherwise8");
      
    // Implicit match
    foreach (i in [1..10])
    {
      | x when x > 5 => Write($"$x");
      | x => Write($"$(x + 1)");
    }
    otherwise
      WriteLine("Otherwise9");
    WriteLine("");
  }
}

