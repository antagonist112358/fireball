using System.Console;

module A
{
 public X(this i : int)                   : void { WriteLine($"i=$i") }
 public X(this i : int * int)             : void { WriteLine($"i=($(i[0]), $(i[1]))") }
 public X(this i : int * int, y : string) : void { WriteLine($"i=($(i[0]), $(i[1])) y=$y") }
}

module Program
{
  Main() : void
  {
    (42).X();
    (1, 2).X();
    (3, 4).X("Test");
  }
}

/*
BEGIN-OUTPUT
i=42
i=(1, 2)
i=(3, 4) y=Test
