using System.Console;

module Program
{
  Main() : void
  {
    mutable s;

    Test1(out s);
    WriteLine(s.F);

    Test2(out s);
    WriteLine(s.F);

    Test3(out s);
    WriteLine(s.F);

    mutable c;

    Test4(out c);
    WriteLine(c.F);

    Test5(out c);
    WriteLine(c.F);

    Test6(out c);
    WriteLine(c.F);

    def a = Test7();
    WriteLine(a.F);
  }

  Test1(s : out S1) : void
  {
    s.F = if(cond) "1" else "2"
  }

  Test2(s : out S1) : void
  {
    s.F = try { if(cond) "1" else "3" } finally { () }
  }

  Test3(s : out S1) : void
  {
    try { s.F = if(cond) "1" else "4" } finally { () }
  }

  Test4(c : out C1) : void
  {
    c = C1();
    c.F = if(cond) "1" else "2"
  }

  Test5(c : out C1) : void
  {
    c = C1();
    c.F = try { if(cond) "1" else "3" } finally { () }
  }

  Test6(c : out C1) : void
  {
    c = C1();
    try { c.F = if(cond) "1" else "4" } finally { () }
  }

  Test7() : S1
  {
    def a = S1();
    a.F = try { if (cond) "1" else "7" } finally { () }
    a
  }

  mutable cond : bool = false;
}

public struct S1
{
  public mutable F : string;
}

public class C1
{
  public mutable F : string;
}

